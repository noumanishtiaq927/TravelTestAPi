{
	"entryFile": "src/index.ts",
	"noImplicitAdditionalProperties": "throw-on-extras",
	"spec": {
		"outputDirectory": "public",
		"specVersion": 3,
		"securityDefinitions": {
			"jwt": {
				"type": "http",
				"scheme": "Bearer",
				"name": "Authorization",
				"in": "header"
			},
			"api_key": {
				"type": "apiKey",
				"name": "adminToken",
				"in": "query"
			}
		}
	}
}
